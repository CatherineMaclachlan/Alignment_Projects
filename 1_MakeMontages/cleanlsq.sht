#!/bin/sh


if (($# == 1))
then
	last=$1
else
	last=$2
fi

for lyr in $(seq $1 $last)
do
	echo $lyr
	if [ -d "$lyr" ]
	then
		cd $lyr

		for jb in $(ls -d * | grep -E 'S[0-9]{1,}_[0-9]{1,}')
		do
			echo $jb
			cd $jb
			rm -f results*
			rm -f triples*
			cd ..
		done

		for jb in $(ls -d * | grep -E 'D[0-9]{1,}_[0-9]{1,}')
		do
			echo $jb
			cd $jb
			rm -f results*
			rm -f triples*
			cd ..
		done

		cd ..
	fi
done
